{
    "manifest_version": 3,
    "name": "Roblox UWP Joiner",
    "version": "3.0",
    "description": "Easily Join Roblox Games on the Roblox UWP version with only your browser! by aspectxyz | .gg/deathsniper",
    "icons": {
        "16": "/assets/16.png",
        "48": "/assets/48.png",
        "128": "/assets/128.png"
    },
    "content_scripts": [
        {
          "matches": ["*://*.roblox.com/games/*"],
          "js": ["/js/index.js"],
          "css": ["/css/style.css"]
        }
    ]  
}